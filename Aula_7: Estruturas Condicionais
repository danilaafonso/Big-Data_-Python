        "soma_notas = sum(lista[2:])\n",
        "\n",
        "nota_descartada = min(lista[2:])\n",
        "\n",
        "soma_notas_validas = soma_notas - nota_descartada\n",
        "\n",
        "media_das_validas = soma_notas_validas/4\n",
        "\n"
