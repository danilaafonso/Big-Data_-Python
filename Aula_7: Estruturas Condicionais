      }
    },
    {
      "cell_type": "code",
      "source": [
        "cont = 1\n",
        "while cont <= 3:\n",
        "  v1 = float(input('Valor 1 = '))\n",
        "  v2 = float(input('Valor 2 = '))\n",
        "\n",
        "  soma = v1+v2\n",
        "  print(soma)\n",
        "  cont = cont + 1"
      ],
      "metadata": {
        "id": "hlMkamJtVQ8F",
        "colab": {
          "base_uri": "https://localhost:8080/"
        },
        "outputId": "feea83fa-3355-4d12-f557-82b41836dedc"
      },
      "execution_count": null,
      "outputs": [
        {
          "output_type": "stream",
          "name": "stdout",
